<hr>
<div class="container">
	<div class="row">
		<%= bootstrap_form_for @setting, url: mystore_setting_path(@setting) do |f| %>
		<%= f.text_field :name %>
		<%= f.select :status, enum_option_pairs(Store, :status) %>
		<%= f.text_field :address %>
		<%= f.text_field :zip %>
		<%= f.text_field :tel %>
		<%= f.submit 'submit' %>
		<% end %>
	</div>
</div>