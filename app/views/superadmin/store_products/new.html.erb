<%= render 'superadmin/share/menu' %>
<div class="container">
	<div class="row">
		Product Nav =>
		<%= link_to 'Back Menu', superadmin_store_products_path %>
	</div>
</div>	
<hr>
<div class="container">
	<div class="row">
		<%= bootstrap_form_for [:superadmin, @store_product] do |f| %>
		<%= f.hidden_field :store_id, value: session[:sid] %>
		<%= f.text_field :name %>
		<%= f.text_field :price %>
		<%= f.text_area :desc %>
		<%= f.submit 'submit' %>
		<% end %>
	</div>
</div>
<%= now_add %>